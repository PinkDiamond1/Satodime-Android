<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@drawable/activity_background"
    android:layout_height="match_parent"
    tools:context=".ui.activity.KeySlotDetailsActivity">


    <androidx.cardview.widget.CardView
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="@dimen/action_bar_size"
        android:elevation="50dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:layout_marginEnd="20dp">

            <ImageView
                android:id="@+id/back_btn"
                android:layout_width="@dimen/icon_size"
                android:layout_height="@dimen/icon_size"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_back"
                app:tint="@color/pure_white" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:layout_centerInParent="true">


                <ImageView
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:src="@drawable/ic_details" />

                <TextView
                    android:id="@+id/heading_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/segoe_ui_bold"
                    android:text="Keslot Info:"
                    android:layout_marginStart="20dp"
                    android:textColor="@color/pure_white"
                    android:textSize="18sp" />
            </LinearLayout>


        </RelativeLayout>
    </androidx.cardview.widget.CardView>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@id/toolbar">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="20dp"
            android:layout_marginEnd="20dp"
            android:orientation="vertical">

            <!-- KEYSLOT INFO -->
            <LinearLayout
                android:id="@+id/group_details_keyslot_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/segoe_ui_bold"
                    android:text="@string/keslot_info"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/item_background_stork"
                    android:orientation="vertical"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="@string/status"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_keyslot_status"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="@string/asset_type"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_keyslot_asset_type"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <TextView
                        android:id="@+id/value_details_keyslot_pubkley"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0x"
                        android:visibility="gone" />

                    <LinearLayout
                        android:id="@+id/show_public_key"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/gold_store_background">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui"
                            android:padding="10dp"
                            android:text="@string/show_public_key"
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>

            <!-- COIN info -->
            <LinearLayout
                android:id="@+id/group_details_coin_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:orientation="vertical">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/segoe_ui_bold"
                    android:text="@string/card_info"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/item_background_stork"
                    android:orientation="vertical"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="@string/blockchain"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_coin_blockchain"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="@string/balance"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_coin_balance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="@string/address"
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_coin_address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"

                        android:layout_gravity="center"
                        android:layout_marginTop="15dp">


                        <androidx.cardview.widget.CardView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginBottom="5dp"
                            android:elevation="5dp"
                            app:cardCornerRadius="5dp">

                            <ImageView
                                android:id="@+id/imbutton_address_copy"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="5dp"
                                android:src="@drawable/ic_copy" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginBottom="5dp"
                            android:elevation="5dp"
                            app:cardCornerRadius="5dp">

                            <ImageView
                                android:id="@+id/imbutton_address_qr"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="5dp"
                                android:src="@drawable/ic_qrcode" />
                        </androidx.cardview.widget.CardView>

                        <androidx.cardview.widget.CardView
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_marginStart="10dp"
                            android:layout_marginBottom="5dp"
                            android:elevation="5dp"
                            app:cardCornerRadius="5dp">

                            <ImageView
                                android:id="@+id/imbutton_address_browse"
                                android:layout_width="match_parent"
                                android:layout_height="match_parent"
                                android:padding="5dp"
                                android:src="@drawable/ic_external_link_alt" />
                        </androidx.cardview.widget.CardView>

                    </LinearLayout>

                    <ImageView
                        android:id="@+id/imview_address_qr"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:contentDescription="qr-code"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginTop="10dp"
                        android:visibility="gone" />
                </LinearLayout>
            </LinearLayout>

            <!-- PRIVATE info (optional)-->
            <LinearLayout
                android:id="@+id/group_details_private_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/segoe_ui_bold"
                    android:text="@string/private_info"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/item_background_stork"
                    android:orientation="vertical"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:background="@drawable/gold_store_background">

                        <TextView
                            android:id="@+id/button_showhide_info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui"
                            android:padding="10dp"
                            android:text="@string/show_more"
                            android:textColor="@color/black"
                            android:textSize="14sp" />


                    </LinearLayout>


                    <LinearLayout
                        android:id="@+id/group_details_private_showhide_info"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/gold_store_background"
                        android:orientation="vertical"
                        android:padding="10dp"
                        android:visibility="gone">

                        <!-- privkey -->
                        <LinearLayout
                            android:id="@+id/group_details_private_key"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content">

                                <TextView
                                    android:id="@+id/text_details_private_key"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/segoe_ui_bold"
                                    android:text="@string/private_key"
                                    android:textColor="@color/black" />

                                <TextView
                                    android:id="@+id/value_details_private_key"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:fontFamily="@font/segoe_ui"
                                    android:text="0xBaf621349202b897efb32a3bo6318"
                                    android:textColor="@color/black"
                                    android:textSize="16sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/group_details_private_key2"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageButton
                                    android:id="@+id/imbutton_privkey_copy"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/outline_content_copy_black_18" />

                                <ImageButton
                                    android:id="@+id/imbutton_privkey_qr"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/outline_qr_code_2_black_18" />

                            </LinearLayout>

                            <ImageView
                                android:id="@+id/imview_privkey_qr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:contentDescription="qr-code"
                                android:visibility="gone" />
                        </LinearLayout>

                        <!-- wif -->
                        <LinearLayout
                            android:id="@+id/group_details_private_wif"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginTop="10dp">

                                <TextView
                                    android:id="@+id/text_details_private_wif"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:fontFamily="@font/segoe_ui_bold"
                                    android:text="@string/wif"
                                    android:textColor="@color/black" />

                                <TextView
                                    android:id="@+id/value_details_private_wif"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="10dp"
                                    android:fontFamily="@font/segoe_ui"
                                    android:text="9942abdd-dsss6524662c4c6e562"
                                    android:textColor="@color/black"
                                    android:textSize="16sp" />
                            </LinearLayout>

                            <LinearLayout
                                android:id="@+id/group_details_private_wif2"
                                android:layout_width="fill_parent"
                                android:layout_height="wrap_content"
                                android:gravity="center"
                                android:orientation="horizontal">

                                <ImageButton
                                    android:id="@+id/imbutton_wif_copy"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/outline_content_copy_black_18" />

                                <ImageButton
                                    android:id="@+id/imbutton_wif_qr"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:src="@drawable/outline_qr_code_2_black_18" />
                            </LinearLayout>

                            <ImageView
                                android:id="@+id/imview_wif_qr"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:contentDescription="qr-code"
                                android:visibility="gone" />
                        </LinearLayout>

                        <!-- entropy -->
                        <LinearLayout
                            android:id="@+id/group_details_private_entropy"
                            android:layout_width="fill_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <TextView
                                android:id="@+id/text_details_private_entropy"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/segoe_ui_bold"
                                android:text="@string/entropy"
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/value_details_private_entropy"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:fontFamily="@font/segoe_ui"
                                android:text="(Entropy in hex)"
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <TextView
                            android:id="@+id/text_details_private_entropy_info"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:text="(Note: ...)" />

                    </LinearLayout>

                </LinearLayout>
            </LinearLayout>

            <!-- TOKEN info (optional)-->
            <LinearLayout
                android:id="@+id/group_details_token_info"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="20dp"
                android:layout_marginTop="20dp"
                android:orientation="vertical"
                android:visibility="gone">
                <TextView
                    android:id="@+id/text_details_token_info"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/segoe_ui_bold"
                    android:text="@string/token_info"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

               <LinearLayout
                   android:layout_width="match_parent"
                   android:orientation="vertical"
                   android:layout_marginTop="10dp"
                   android:background="@drawable/item_background_stork"
                   android:padding="20dp"
                   android:layout_height="wrap_content">
                   <LinearLayout
                       android:id="@+id/group_details_token_contract"
                       android:layout_width="fill_parent"

                       android:layout_height="wrap_content"
                       android:orientation="horizontal">
                       <TextView
                           android:id="@+id/text_details_token_contract"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:fontFamily="@font/segoe_ui_bold"
                           android:text="@string/token"
                           android:textColor="@color/black" />
                       <TextView
                           android:id="@+id/value_details_token_contract"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginStart="10dp"
                           android:fontFamily="@font/segoe_ui"
                           android:text="0x..."
                           android:textColor="@color/black"
                           android:textSize="16sp" />
                   </LinearLayout>

                   <LinearLayout
                       android:id="@+id/group_details_token_balance"
                       android:layout_width="fill_parent"
                       android:layout_marginTop="10dp"
                       android:layout_height="wrap_content"
                       android:orientation="horizontal">
                       <TextView
                           android:id="@+id/text_details_token_balance"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:fontFamily="@font/segoe_ui_bold"
                           android:text="@string/token_balance"
                           android:textColor="@color/black" />
                       <TextView
                           android:id="@+id/value_details_token_balance"
                           android:layout_width="wrap_content"
                           android:layout_height="wrap_content"
                           android:layout_marginStart="10dp"
                           android:fontFamily="@font/segoe_ui"
                           android:text="(unknown token balance)"
                           android:textColor="@color/black"
                           android:textSize="16sp" />
                   </LinearLayout>
               </LinearLayout>

            </LinearLayout>

            <!-- NFT info (optional)-->
            <LinearLayout
                android:id="@+id/group_details_nft_info"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="20dp"
                android:layout_marginBottom="50dp"
                android:orientation="vertical"
                android:visibility="gone">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/segoe_ui_bold"
                    android:text="@string/nft_info"
                    android:textColor="@color/black"
                    android:textSize="20sp" />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="10dp"
                    android:background="@drawable/item_background_stork"
                    android:orientation="vertical"
                    android:padding="20dp">

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="Contract: "
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_nft_contract"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="Token ID: "
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_nft_tokenid"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp">

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui_bold"
                            android:text="Balance: "
                            android:textColor="@color/black" />

                        <TextView
                            android:id="@+id/value_details_nft_balance"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:fontFamily="@font/segoe_ui"
                            android:text=""
                            android:textColor="@color/black"
                            android:textSize="16sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="15dp"
                        android:background="@drawable/gold_store_background">

                        <TextView
                            android:id="@+id/show_more_btn"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:fontFamily="@font/segoe_ui"
                            android:padding="10dp"
                            android:text="Show More"
                            android:textColor="@color/black"
                            android:textSize="14sp" />
                    </LinearLayout>

                    <LinearLayout
                        android:id="@+id/nft_info_layout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="10dp"
                        android:background="@drawable/gold_store_background"
                        android:orientation="vertical"
                        android:padding="10dp"
                        android:visibility="gone">
                        
                        <LinearLayout
                            android:id="@+id/group_details_nft_name"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp">
                            
                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/segoe_ui_bold"
                                android:text="Name: "
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/value_details_nft_name"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:fontFamily="@font/segoe_ui"
                                android:text=""
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </LinearLayout>
                        
                        <LinearLayout
                            android:id="@+id/group_details_nft_description"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content">

                            <TextView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:fontFamily="@font/segoe_ui"
                                android:text="Description: "
                                android:textColor="@color/black" />

                            <TextView
                                android:id="@+id/value_details_nft_description"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="10dp"
                                android:fontFamily="@font/segoe_ui"
                                android:text=""
                                android:textColor="@color/black"
                                android:textSize="16sp" />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imview_nft_image"
                            android:layout_width="150dp"
                            android:layout_height="150dp"
                            android:visibility="gone"
                            android:layout_gravity="center"
                            android:src="@drawable/content_writing" />
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>


        </LinearLayout>
    </ScrollView>




</RelativeLayout>